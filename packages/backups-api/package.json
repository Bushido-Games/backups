    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nanoid": "^3.3.7",
    "node-schedule": "^2.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "src": "link:./src"
  },
