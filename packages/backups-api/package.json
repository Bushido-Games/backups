  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn nest build",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development yarn nest start --watch",
    "start:debug": "NODE_ENV=development yarn nest start --debug --watch",
    "start:prod": "NODE_ENV=production yarn node dist/src/main",
    "lint": "yarn eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "yarn prettier src",
    "format:check": "yarn prettier src --check",
    "format:fix": "yarn format --write",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "check:types": "yarn nest build -p ./tsconfig.check.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.288.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
